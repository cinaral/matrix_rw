{
	"configurations": [
		{
			"name": "gdb",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}\\build\\bin\\${fileBasenameNoExtension}.exe",
			"cwd": "${workspaceFolder}\\build\\bin",
			"preLaunchTask": "Build",
			"setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
			]
		}
	],
	"version": "0.2.0"
}
